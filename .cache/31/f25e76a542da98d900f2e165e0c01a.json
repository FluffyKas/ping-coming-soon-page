ondary-blue: hsl(223, 100%, 88%);\r\n$clr-secondary-red: hsl(354, 100%, 66%);\r\n\r\n// NEUTRAL COLORS\r\n\r\n$clr-neutral-gray: hsl(0, 0%, 59%);\r\n$clr-neutral-blue: hsl(209, 33%, 12%);\r\n\r\n* {\r\n  box-sizing: border-box;\r\n  margin: 0;\r\n}\r\n\r\nbody {\r\n  font-family: 'Libre Franklin', sans-serif;\r\n  font-size: 20px;\r\n  text-align: center;\r\n}\r\n\r\nmain {\r\n  margin: 0 auto;\r\n  max-width: 900px;\r\n  padding-top: 5rem;\r\n  padding-left: 2rem;\r\n  padding-right: 2rem;\r\n}\r\n\r\n.title {\r\n  color: $clr-neutral-gray;\r\n  font-size: 1.8rem;\r\n  font-weight: 300;\r\n  padding-top: 3rem;\r\n  padding-bottom: 1.5rem;\r\n\r\n  @media (min-width: 760px) {\r\n    font-size: 3rem;\r\n  }\r\n}\r\n\r\n.title-highlight {\r\n  color: $clr-neutral-blue;\r\n  font-weight: 700;\r\n}\r\n\r\n.sub-title {\r\n  color: $clr-neutral-blue;\r\n  font-size: 1rem;\r\n  font-weight: 300;\r\n  padding-bottom: 2.5rem;\r\n\r\n  @media (min-width: 760px) {\r\n    font-size: 1.2rem;\r\n  }\r\n}\r\n\r\nform {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 1rem;\r\n  justify-content: center;\r\n  margin: 0 auto;\r\n  max-width: 480px;\r\n  padding: 0 1rem;\r\n  position: relative;\r\n\r\n  @media (min-width: 860px) {\r\n    flex-direction: row;\r\n    max-width: 100%;\r\n  }\r\n\r\n  input {\r\n    all: unset;\r\n    border: 1px solid $clr-secondary-blue;\r\n    border-radius: 35px;\r\n    color: $clr-neutral-gray;\r\n    font-size: 1rem;\r\n    padding: 1rem 2rem;\r\n    text-align: left;\r\n    \r\n    @media (min-width: 860px) {\r\n      width: 50%;\r\n    }\r\n  }\r\n\r\n  button {\r\n    all: unset;\r\n    background: $clr-primary-blue;\r\n    border-radius: 35px;\r\n    color: white;\r\n    cursor: pointer;\r\n    font-size: 1rem;\r\n    font-weight: 600;\r\n    padding: 1rem 2rem;\r\n\r\n    @media (min-width: 860px) {\r\n      padding: 1rem 3.5rem;\r\n    }\r\n  }\r\n}\r\n\r\n.error-message {\r\n  color: $clr-secondary-red;\r\n  font-size: .75rem;\r\n  font-style: italic;\r\n  position: absolute;\r\n  top: 3.5rem;\r\n  left: 2rem;\r\n  visibility: hidden;\r\n\r\n  @media (min-width: 860px) {\r\n    top: 3.7rem;\r\n    left: 6rem;\r\n  }\r\n}\r\n\r\n.error-message.error{\r\n  visibility: visible;\r\n}\r\n\r\n.mockup {\r\n  display: block;\r\n  max-width: 100%;\r\n  height: auto;\r\n  margin: 5rem auto 10rem auto;\r\n\r\n  @media (min-width: 900px) {\r\n    max-width: 650px;\r\n    margin: 5rem auto 4rem auto;\r\n  }\r\n}\r\n\r\n// FOOTER\r\n\r\n.social-media {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: center;\r\n  gap: 1.1rem;\r\n  margin-bottom: 2rem;\r\n  \r\n  a {\r\n    border: 1px solid $clr-secondary-blue;\r\n    border-radius: 50%;\r\n    color: $clr-primary-blue;\r\n    font-size: .9rem;\r\n    text-decoration: none;\r\n    transition: all .2s ease-in;\r\n  }\r\n\r\n  a:hover {\r\n    background: $clr-primary-blue;\r\n    color: white;\r\n  }\r\n}\r\n\r\n.fa-facebook-f {\r\n  padding: .6rem .85rem;\r\n}\r\n\r\n.fa-twitter {\r\n  padding: .6rem .6rem;\r\n}\r\n\r\n.fa-instagram {\r\n  padding: .6rem .65rem;\r\n}\r\n\r\n.copyright {\r\n  color: $clr-neutral-gray;\r\n  font-size: .85rem;\r\n  margin-bottom: 2rem;\r\n}"},"lineCount":null}},"error":null,"hash":"ce1ebe4feb1a3f7c5b8a5fb39febb80b","cacheData":{"env":{}}}